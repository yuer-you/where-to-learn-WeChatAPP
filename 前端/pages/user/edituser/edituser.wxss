/* pages/edituser/edituser.wxss */
.userPhoto {
    margin: 20rpx;
    background-color: #fff;
}
.userPhoto image {
    width: 120rpx;
    height: 120rpx;
    border-radius: 20rpx;
}

.userName {
    margin: 10rpx 0 100rpx 0;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}
.userName input {
    padding: 10rpx 20rpx;
    border: 1px solid #555;
    border-radius: 14rpx;
}