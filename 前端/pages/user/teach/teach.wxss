/* pages/user/user.wxss */
.all {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

.body {
    padding: 90rpx 60rpx 60rpx 60rpx;
}
.body .setGroup {
    width: 100%;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: flex-start;
}
.setGroup .set {
    width: 100%;
    margin-bottom: 85rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    position:relative
}
.set .right {
    width: 30rpx;
    height: 30rpx;
    margin-right: 20rpx;
    position:absolute;
    right:0
}

.adv {
    margin: 50rpx 0;
}